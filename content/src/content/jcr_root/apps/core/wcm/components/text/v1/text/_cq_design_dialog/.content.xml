<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Text Configuration"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <plugins
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <Features
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Features"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maimized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <edit
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <paste-plaintext
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="paste text"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            name="paste-plaintext"/>
                                                    <paste-wordhtml
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="paste from word"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            name="paste-wordhtml"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        name="edit"
                                                        plugin-root-name="rtePlugins"/>
                                            </edit>
                                            <find-replace
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <find-replace
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="find &amp; replace"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                            features="*"
                                                            name="findreplace"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        plugin-root-name="rtePlugins"/>
                                            </find-replace>
                                            <spellcheck
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <spellcheck
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="spell checker"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                            features="*"
                                                            name="spellcheck"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        plugin-root-name="rtePlugins"/>
                                            </spellcheck>
                                            <editHTML
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <editHTML
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="edit HTML"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            name="sourceedit"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        name="misctools"
                                                        plugin-root-name="rtePlugins"/>
                                            </editHTML>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </Features>
                    <Formatting
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Formatting"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <align
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <align
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="align"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                            defaultEnabled="{Boolean}true"
                                                            features="*"
                                                            name="justify"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        plugin-root-name="rtePlugins"/>
                                            </align>
                                            <format
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <bold
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="bold"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="bold"/>
                                                    <italic
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="italic"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="italic"/>
                                                    <underline
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="underline"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="underline"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        name="format"
                                                        plugin-root-name="rtePlugins"/>
                                            </format>
                                            <links
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <link
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="link text"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="modifylink"/>
                                                    <unlink
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="unlink text"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="unlink"/>
                                                    <anchor
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="anchor"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="anchor"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        name="links"
                                                        plugin-root-name="rtePlugins"/>
                                            </links>
                                            <subsuperscript
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <superscript
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="superscript"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            name="superscript"/>
                                                    <subscript
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="subscript"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            name="subscript"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        name="subsuperscript"
                                                        plugin-root-name="rtePlugins"/>
                                            </subsuperscript>
                                            <lists
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <unordered
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="bullet list"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="unordered"/>
                                                    <ordered
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="number list"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                            defaultEnabled="{Boolean}true"
                                                            name="ordered"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        name="lists"
                                                        plugin-root-name="rtePlugins"/>
                                            </lists>
                                            <tables
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <tables
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="tables"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                            features="*"
                                                            name="table"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        plugin-root-name="rtePlugins"/>
                                            </tables>
                                            <images
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <images
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="images"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                            features="*"
                                                            name="image"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        plugin-root-name="rtePlugins"/>
                                            </images>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </Formatting>
                    <Paraformat
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Paragraph styles"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <paraformat
                                            granite:class="js-cq-IPEPlugin-group"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Enable Paragraph formats"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                    features="*"
                                                    name="paraformat"/>
                                            <configWrapper
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/wrapper"
                                                    configPath="./rtePlugins/paraformat/formats">
                                                <formats
                                                        granite:class="cq-paragraphstyles"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Custom formats">
                                                    <field
                                                            granite:class="cq-paragraphstyles-field"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./rtePlugins/paraformat/formats">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <tag
                                                                    granite:class="cq-rte-symbol"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    emptyText="tag"
                                                                    name="tag"/>
                                                            <description
                                                                    granite:class="cq-rte-symbol-description"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    emptyText="description"
                                                                    name="description"/>
                                                        </items>
                                                    </field>
                                                </formats>
                                            </configWrapper>
                                        </items>
                                        <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                plugin-root-name="rtePlugins"/>
                                    </paraformat>
                                </items>
                            </fixedcol>
                        </items>
                    </Paraformat>
                    <Characters_x0020_styles
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Characters Styles"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <characters-styles
                                            granite:class="js-cq-IPEPlugin-group"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Enable styles"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                    features="*"
                                                    name="styles"/>
                                            <accordion
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <styleSheetsConfigWrapper
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Add paths of external style sheets"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/wrapper"
                                                            configPath="./externalStyleSheets">
                                                        <styleSheets
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    name="./externalStyleSheets"/>
                                                        </styleSheets>
                                                    </styleSheetsConfigWrapper>
                                                    <stylesConfigWrapper
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Add styles"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/wrapper"
                                                            configPath="./rtePlugins/styles/styles">
                                                        <styles
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./rtePlugins/styles/styles">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <cssName
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            emptyText="cssName"
                                                                            name="cssName"/>
                                                                    <text
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            emptyText="Display text"
                                                                            name="text"/>
                                                                </items>
                                                            </field>
                                                        </styles>
                                                    </stylesConfigWrapper>
                                                </items>
                                            </accordion>
                                        </items>
                                        <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                plugin-root-name="rtePlugins"/>
                                    </characters-styles>
                                </items>
                            </fixedcol>
                        </items>
                    </Characters_x0020_styles>
                    <Characters
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Characters"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <characters
                                            granite:class="js-cq-IPEPlugin-group"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <specialchars
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Enable Special characters"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                    features="specialchars"
                                                    name="misctools"/>
                                            <configWrapper
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/wrapper"
                                                    configPath="./rtePlugins/misctools/specialCharsConfig/chars">
                                                <specialCharsConfig
                                                        granite:class="cq-specialcharacters"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Add characters">
                                                    <field
                                                            granite:class="cq-specialcharacters-field"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./rtePlugins/misctools/specialCharsConfig/chars">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <entity
                                                                    granite:class="cq-rte-symbol"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    emptyText="entity"
                                                                    name="entity"/>
                                                            <name
                                                                    granite:class="cq-rte-symbol-description"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    emptyText="name"
                                                                    name="name"/>
                                                        </items>
                                                    </field>
                                                </specialCharsConfig>
                                            </configWrapper>
                                        </items>
                                        <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                plugin-root-name="rtePlugins"/>
                                    </characters>
                                </items>
                            </fixedcol>
                        </items>
                    </Characters>
                </items>
            </plugins>
        </items>
    </content>
</jcr:root>
